# Production Environment Variables for Render Deployment
# Copy these to your Render environment variables

NODE_ENV=production

# Database Configuration (Required)
DATABASE_URL=your_postgresql_connection_string_here

# Supabase Configuration (Required)
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Optional API Keys for Enhanced Features
OPENAI_API_KEY=your_openai_api_key_here
MYSHIPTRACKING_API_KEY=your_myshiptracking_api_key_here
MARINE_TRAFFIC_API_KEY=your_marine_traffic_api_key_here

# Server Configuration
PORT=5000